node {
  
        stage("yarn install ") {
         
          sh'yarn install'
           
        }
    }



node {
  
        stage("docker-compose build ") {
         
          sh'docker-compose build'
           
        }
    }


node {
  
        stage("docker-compose up ") {
       
          sh'docker-compose up'
           
        }
    }
